{
  "name": "react-yue",
  "version": "0.4.0",
  "description": "This is a lib to help you render the View of Yue in the react way.",
  "main": "lib/index.js",
  "scripts": {
    "example": "node lib/__tests__/app.js",
    "build_lib": "babel src --out-dir lib -w",
    "build": "babel src --out-dir lib",
    "jest": "jest",
    "test": "npm run build && node test.js"
  },
  "author": "oyyd <oyydoibh@gmail.com>",
  "files": [
    "lib",
    "README.md"
  ],
  "peerDependencies": {
    "gui": ">=0.4.4",
    "react": "^16.2.0"
  },
  "dependencies": {
    "react-reconciler": "^0.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "gui": "0.4.3",
    "jest": "^22.0.3",
    "react": "^16.2.0"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "testMatch": [
      "**/lib/**/__tests__/**/?(*.)+(spec|test).js?(x)"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "jest": true
    },
    "extends": "airbnb",
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "arrow-parens": 0,
      "global-require": 0,
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": 0,
      "jsx-a11y/label-has-for": 0
    }
  }
}
